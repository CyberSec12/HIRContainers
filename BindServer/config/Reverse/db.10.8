;
; BIND reverse data file for local loopback interface
;
$TTL	604800
@	IN	SOA	webserver.ex. root.webserver.ex. (
			      3		; Serial
			 604800		; Refresh
			  86400		; Retry
			2419200		; Expire
			 604800 )	; Negative Cache TTL
;
@       IN	NS	localhost.
10.3 	IN 	PTR webserver.ex
10.21	IN	PTR webgoat.local
10.22	IN	PTR	baadstore.local
10.23	IN	PTR	dwa.local
10.24	IN	PTR	mutillidae.local
10.25	IN	PTR	bwap.local
10.26	IN	PTR	nessus.local
10.27	IN	PTR	wordpress.local
10.50	IN	PTR	nhm.local
